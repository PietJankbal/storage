load_cinepak()
{
    w_download "http://www.probo.com/pub/cinepak/cvid32.zip" a41984a954fe77557f228fa8a95cdc05db22bf9ff5429fe4307fd6fc51e11969

    if [ -f "$W_SYSTEM32_DLLS/iccvid.dll" ]; then
        w_try rm -f "$W_SYSTEM32_DLLS/iccvid.dll"
    fi

    w_try_unzip "$W_SYSTEM32_DLLS" "${W_CACHE}/${W_PACKAGE}/${file1}" ICCVID.DLL

    w_try mv -f "$W_SYSTEM32_DLLS/ICCVID.DLL" "$W_SYSTEM32_DLLS/iccvid.dll"

    w_override_dlls native iccvid
}

#----------------------------------------------------------------

w_metadata jet40 dlls \
    title="MS Jet 4.0 Service Pack 8" \
    publisher="Microsoft" \
    year="2003" \
    media="download" \
    file1="jet40sp8_9xnt.exe" \
    installed_file1="$W_COMMONFILES_WIN/Microsoft Shared/dao/dao360.dll"

load_jet40()
{
    # mdac27 is 32-bit only, so use mdac28 for win64:
    if [ "$W_ARCH" = "win64" ] ; then
        w_call mdac28
    else
        w_call mdac27
    fi
    w_call wsh57

    # https://support.microsoft.com/kb/239114
    # See also https://bugs.winehq.org/show_bug.cgi?id=6085
    # FIXME: "failed with error 2"
    w_download https://download.microsoft.com/download/4/3/9/4393c9ac-e69e-458d-9f6d-2fe191c51469/jet40sp8_9xnt.exe b060246cd499085a31f15873689d5fa7df817e407c8261a5c71fa6b9f7042560

    w_try_cd "$W_CACHE/$W_PACKAGE"
    w_try "$WINE" jet40sp8_9xnt.exe $W_UNATTENDED_SLASH_Q
}

# FIXME: verify_jet40()
# See https://github.com/Winetricks/winetricks/issues/327,
# https://en.wikibooks.org/wiki/JET_Database/Creating_and_connecting, and
# https://msdn.microsoft.com/en-us/library/ms677200%28v=vs.85%29.aspx

#----------------------------------------------------------------

w_metadata ie8_kb2936068 dlls \
    title="Cumulative Security Update for Internet Explorer 8" \
    publisher="Microsoft" \
    year="2014" \
    media="download" \
    file1="IE8-WindowsXP-KB2936068-x86-ENU.exe" \
    installed_file1="c:/windows/KB2936068-IE8.log"

load_ie8_kb2936068()
{
    # If we really need win64 support, should check if there's an x64 version of the hotfix
    w_package_unsupported_win64

    w_call ie8

    w_download https://download.microsoft.com/download/3/8/C/38CE0ABB-01FD-4C0A-A569-BC5E82C34A17/IE8-WindowsXP-KB2936068-x86-ENU.exe 8bda23c78cdcd9d01c364a01c6d639dfb2d11550a5521b8a81c808c1a2b1824e

    if [ -n "$W_UNATTENDED_SLASH_Q" ]; then
        quiet="$W_UNATTENDED_SLASH_QUIET /forcerestart"
    else
        quiet=""
    fi

    w_set_winver winxp

    w_try_cd "$W_CACHE/$W_PACKAGE"
    "$WINE" IE8-WindowsXP-KB2936068-x86-ENU.exe $quiet
    status=$?
    case $status in
        0|194) ;;
        *) w_die "$W_PACKAGE installation failed"
    esac

    w_set_winver 'default'
}






############################################################################################################################################

#----------------------------------------------------------------

helper_winvista()
{
    filename=$1

    w_download_to winvista https://download.microsoft.com/download/8/3/b/83b8c814-b000-44a4-b667-8c1f58727b8b/Windows6.0-KB936330-X64-wave0.exe a48ec1ba10120676c0529121dff5303ec777988ba25133d1dd15234a04dd6f9e

    cabextract -d "$W_TMP/winvista" -F "$filename" "$W_CACHE/winvista/Windows6.0-KB936330-X64-wave0.exe"
}


#----------------------------------------------------------------

w_metadata expand dlls \
    title="expand.exe; can be used to extract files from .msu package" \
    publisher="Microsoft" \
    media="download" \
    file1="Windows6.0-KB936330-X64-wave0.exe" \
    installed_file1="$W_SYSTEM32_DLLS_WIN/expand.exe"

load_expand()
{
    for i in x86_microsoft-windows-basic-misc-tools_31bf3856ad364e35_6.0.6001.18000_none_175cb770bf6b8f77/expand.exe \
        x86_microsoft-windows-deltapackageexpander_31bf3856ad364e35_6.0.6001.18000_none_69de962f3507db4b/dpx.dll \
        x86_microsoft-windows-cabinet_31bf3856ad364e35_6.0.6001.18000_none_373f511ce1ebb446/cabinet.dll \
        x86_microsoft-windows-servicingstack_31bf3856ad364e35_6.0.6001.18000_none_095f6148c74a7a64/msdelta.dll \
        x86_microsoft-windows-d..txvideoacceleration_31bf3856ad364e35_6.0.6001.18000_none_6bd4b28e4232f018/dxva2.dll; do

        helper_winvista "$i"
        w_try cp "$W_TMP/winvista/$i" "$W_SYSTEM32_DLLS/"

    done

    if [ "$W_ARCH" = "win64" ]; then

        for i in amd64_microsoft-windows-basic-misc-tools_31bf3856ad364e35_6.0.6001.18000_none_737b52f477c900ad/expand.exe \
            amd64_microsoft-windows-deltapackageexpander_31bf3856ad364e35_6.0.6001.18000_none_c5fd31b2ed654c81/dpx.dll \
            amd64_microsoft-windows-cabinet_31bf3856ad364e35_6.0.6001.18000_none_935deca09a49257c/cabinet.dll \
            amd64_microsoft-windows-deltacompressionengine_31bf3856ad364e35_6.0.6001.18000_none_9c4b039c3e1c8af8/msdelta.dll \
            amd64_microsoft-windows-d..txvideoacceleration_31bf3856ad364e35_6.0.6001.18000_none_c7f34e11fa90614e/dxva2.dll; do

            helper_winvista "$i"
            w_try cp "$W_TMP/winvista/$i" "$W_SYSTEM64_DLLS/"
        done
    fi

    #Do not set cabinet to native, it breaks lots of installers (!), but to extract files with expand it works ok. So give example to user
    echo "Example of usage: WINEDLLOVERRIDES=expand.exe,cabinet=n wine expand -F:* some.msu .  ---> extract all files from msu to current dir
                            WINEDLLOVERRIDES=expand.exe,cabinet=n wine expand -F:* some.cab .  ---> extract all files from cab to current dir
                            WINEDLLOVERRIDES=expand.exe,cabinet=n wine expand /d some.cab      ---> list all files in cab"
}

#----------------------------------------------------------------



w_metadata wmf_refresh dlls \


load_wmf_refresh()
{
msuname='Windows6.1-KB968211-x64-RefreshPkg.msu'
cabname='Windows6.1-KB968211-x64-RefreshPkg.cab'
downloadlink='https://download.microsoft.com/download/C/1/E/C1E5D151-A6CA-480C-85A2-1554C4E6A22B/Windows6.1-KB968211-x64-RefreshPkg.msu'


  w_download_to $W_PACKAGE "$downloadlink" # b7976e24d19d2aade5491514b9af3aeee6d095c774eec846fbbc21348389d593


    w_try mkdir "$W_TMP/$W_PACKAGE/"
    w_try cd "$W_TMP/$W_PACKAGE/"

 echo $W_PACKAGE
    w_call expand

    for i in ${wmf16_refreshvalues}; do

    case $i in
             _^skip^_*)
             echo "skipping"
             ;;

             x86* | wow64*) helper_win7sp2 $i $W_PACKAGE $msuname $cabname $downloadlink
             reghelper $i $W_PACKAGE $cabname

             w_try cp -vf "$W_TMP"/$i "$W_SYSTEM32_DLLS/"
             ;;

             amd64*) helper_win7sp2 $i $W_PACKAGE $msuname $cabname $downloadlink
             reghelper $i $W_PACKAGE $cabname

             w_try cp -vf "$W_TMP"/$i "$W_SYSTEM64_DLLS/"
             ;;

             *) w_warn "Nothing done" ;;
           esac
    done


}

