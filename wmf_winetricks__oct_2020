w_metadata wmvcore dlls \
    title="MS wmvcore.dll" \
    publisher="Microsoft" \
    year="2011" \
    media="download" \
    file1="../win7sp1/windows6.1-KB976932-X86.exe" \
    installed_file1="$W_SYSTEM32_DLLS_WIN/wmvcore.dll"

load_wmvcore()
{
    helper_win7sp1 x86_microsoft-windows-mediaplayer-wmvcore_31bf3856ad364e35_6.1.7601.17514_none_0939edb934199a4a/wmvcore.dll
    w_try cp "$W_TMP/x86_microsoft-windows-mediaplayer-wmvcore_31bf3856ad364e35_6.1.7601.17514_none_0939edb934199a4a/wmvcore.dll" "$W_SYSTEM32_DLLS/wmvcore.dll"

    w_override_dlls native,builtin wmvcore

    w_try_regsvr wmvcore.dll

    if [ "$W_ARCH" = "win64" ]; then
        helper_win7sp1_x64 amd64_microsoft-windows-mediaplayer-wmvcore_31bf3856ad364e35_6.1.7601.17514_none_6558893cec770b80/wmvcore.dll
        w_try cp "$W_TMP/amd64_microsoft-windows-mediaplayer-wmvcore_31bf3856ad364e35_6.1.7601.17514_none_6558893cec770b80/wmvcore.dll" "$W_SYSTEM64_DLLS/wmvcore.dll"
        w_try_regsvr64 wmvcore.dll
    fi
}

#----------------------------------------------------------------

w_metadata wmf_install dlls

load_wmf_install()
{


    w_call win7sp2

    w_call wmf_refresh

    w_call mf_register

    w_call wmvcore

    w_override_dlls native wmvcore.dll wmp.dll mf mfreadwrite mfplay mfplat dxva2


    w_try_cd "$W_SYSTEM64_DLLS"
    for i in wmvcore.dll wmp.dll colorcnv.dll msmpeg2adec.dll msmpeg2vdec.dll; do
        w_try_regsvr64 $i
    done

    w_try_cd "$W_SYSTEM32_DLLS"
    for i in wmvcore.dll wmp.dll colorcnv.dll msmpeg2adec.dll msmpeg2vdec.dll; do
        w_try_regsvr $i
    done
}

helper_win7sp2()
{
    filename=$1
    packagename=$2
    msuname=$3
    cabname=$4
    downloadlink=$5



    if test -f "$W_CACHE/$packagename/$msuname"; then
        echo "windows6.1-kb3125574-v4-x64_2dafb1d203c8964239af3048b5dd4b1264cd93b9.msu exists, skipping download ...."
    else
        w_download_to $packagename "$downloadlink" # b7976e24d19d2aade5491514b9af3aeee6d095c774eec846fbbc21348389d593
    fi

    if test -f "$W_CACHE/$packagename/$cabname"; then
        echo "Windows6.1-KB3125574-v4-x64.cab exists, skipping cabextract ...."
    else
        w_try cabextract -d "$W_CACHE/$packagename" -F "$cabname" "$W_CACHE/$packagename/$msuname"
    fi

    STRIPPEDNAME="$(echo $filename | sed -n 's/^.*\//\//p' | sed 's/\///')"
    echo "Processing $STRIPPEDNAME"
    WINEDLLOVERRIDES=expand.exe,cabinet=n "$WINE" expand.exe "$W_CACHE_WIN\\$packagename\\$cabname" /f:"$STRIPPEDNAME" "$W_TMP_WIN"
}

# A crappy attempt to turn manifest --> reg ......
do_sed_voodoo()
{
  manifestxml="$1"
  cat "$manifestxml" \
| sed '/<registryKeys>/,$!d' | sed '/<\/registryKeys>/,$d' \
| sed 's/<registryKey keyName="HKEY_/[HKEY_/g'| sed 's/<registryValue name=//g' \
| sed 's/owner="false" \/>//g'| sed 's/owner="true" \/>//g' \
| sed 's/" owner="false".*>/]/g' \
| sed 's/ operationHint="replace"//g' \
| sed '/REG_SZ/ s/\\/\\\\/g' \
| sed 's/ valueType="REG_SZ" value=/=/g'| sed 's/ owner="true" >//g' \
| sed '/REG_EXPAND_SZ/ s/\\/\\\\/g' \
| sed 's/ valueType="REG_EXPAND_SZ" value=/=/g' \
| sed 's/ valueType="REG_BINARY" value="/=hex:/g' \
| sed 's/ valueType="REG_DWORD" value="/=dword:/g' \
| sed 's/ valueType="REG_QWORD" value="/=dword:/g' \
| sed 's/ valueType="REG_NONE" value=/=/g' \
| sed 's/<registryKey>//g' \
| sed 's/<\/registryKey>//g' \
| sed '/hex:.*/ s/[0-9A-F][0-9A-F]/&,/g; s/,$//' \
| sed '/hex:.*/ s/,"//' \
| sed 's/<registryKeys>/REGEDIT4/' \
| sed 's/""=/@=/' \
| sed 's/^[ \t]*//' \
| sed '/HKEY_/ s/\\]/]/' | sed '/HKEY_/ s/"/]/' \
| sed 's/\r//g' \
| sed 's/\s*$//g' \
| sed '/dword:.*/ {s/.$//g}' \
| sed '/securityDescriptor name/d' \
| sed 's/%systemroot%/c:\\\\windows/Ig' \
| sed '/MULTI_SZ/ s/ valueType="REG_MULTI_SZ" value="&quot;/=hex(7):/g' \
| perl -pe 's/(hex\(7\):)(.*)/sprintf "%s%s", $1, join(",", unpack("(H2)*", $2))/pe;'   | sed '/--Leave a comment here--/ s/-/-/' \
| sed '/hex(7)/ s/26,71,75,6f,74,3b,2c,/00/g' \
| sed '/hex(7)/ s/26,71,75,6f,74,3b//g' \
| sed '/hex(7)/ s/,,22/,00,00/g' \
| sed 's/&quot;//g'                                                                     | sed '/--FIXME, do we need to replace &quot; with quotationmarks or space??--/ s/-/-/'
}



reghelper()
{
    filename=$1
    packagename=$2
    cabname=$3
if ! test -d "$W_TMP/$packagename"; then
w_try mkdir "$W_TMP/$packagename/"
fi
    MANIFESTNAME="$(echo $filename | sed 's/\/.*//').manifest"
echo "Processing $MANIFESTNAME in $W_TMP_WIN\\$packagename\\"
WINEDLLOVERRIDES=expand.exe,cabinet=n "$WINE" expand.exe "$W_CACHE_WIN\\$packagename\\$cabname" /f:"$MANIFESTNAME" "$W_TMP_WIN\\$packagename\\"


tput sgr0
tput setaf 1
tput bold
do_sed_voodoo $W_TMP/$packagename/$MANIFESTNAME | tee "$W_TMP/$packagename/$MANIFESTNAME".reg #| tee "$W_CACHE/$packagename/$MANIFESTNAME".reg


tput setaf 5

case $MANIFESTNAME in
  x86* | wow64*) sed -i 's/$(runtime.system32)/c:\\\\windows\\\\syswow64/g' "$W_TMP/$packagename/$MANIFESTNAME".reg
  w_try_regedit32 "$W_TMP/$packagename/$MANIFESTNAME".reg
  ;;

  amd64*) sed -i 's/$(runtime.system32)/c:\\\\windows\\\\system32/g' "$W_TMP/$packagename/$MANIFESTNAME".reg
  w_try_regedit64 "$W_TMP/$packagename/$MANIFESTNAME".reg
  ;;
esac
tput sgr0
}


w_metadata win7sp2 dlls

load_win7sp2()
{

    w_call expand

    w_try mkdir "$W_TMP/$W_PACKAGE/"

    for i in ${win7sp2values}; do

    case $i in
             _^skip^_*)
             echo "skipping"
             ;;
             x86* | wow64*) helper_win7sp2 $i $W_PACKAGE "windows6.1-kb3125574-v4-x64_2dafb1d203c8964239af3048b5dd4b1264cd93b9.msu" "Windows6.1-KB3125574-v4-x64.cab" "http://download.windowsupdate.com/d/msdownload/update/software/updt/2016/05/windows6.1-kb3125574-v4-x64_2dafb1d203c8964239af3048b5dd4b1264cd93b9.msu"
             reghelper $i $W_PACKAGE "Windows6.1-KB3125574-v4-x64.cab"
             w_try cp -vf "$W_TMP"/$i "$W_SYSTEM32_DLLS/"
             ;;

             amd64*) helper_win7sp2 $i $W_PACKAGE "windows6.1-kb3125574-v4-x64_2dafb1d203c8964239af3048b5dd4b1264cd93b9.msu"  "Windows6.1-KB3125574-v4-x64.cab" http://download.windowsupdate.com/d/msdownload/update/software/updt/2016/05/windows6.1-kb3125574-v4-x64_2dafb1d203c8964239af3048b5dd4b1264cd93b9.msu
             reghelper $i $W_PACKAGE "Windows6.1-KB3125574-v4-x64.cab"
             w_try cp -vf "$W_TMP"/$i "$W_SYSTEM64_DLLS/"
             ;;

             *) w_warn "Nothing done" ;;
           esac
    done


}

w_metadata mf_register dlls \

load_mf_register()
{

#MF.REG
cat > "$W_TMP"/mf.reg <<_EOF_
REGEDIT4

[HKEY_LOCAL_MACHINE\Software\Wine\LicenseInformation]
"msmpeg2adec-AACDecoderV2AddInEnable"=dword:00000001
"msmpeg2adec-AACDecoderV2InSKU"=dword:00000001
"msmpeg2adec-DolbyDigitalDecoderV2AddInEnable"=dword:00000001
"msmpeg2adec-DolbyDigitalDecoderV2InSKU"=dword:00000001
"msmpeg2vdec-H264VideoDecoderV2AddInEnable"=dword:00000001
"msmpeg2vdec-H264VideoDecoderV2InSKU"=dword:00000001
"msmpeg2vdec-MPEG2VideoDecoderV2AddInEnable"=dword:00000001
"msmpeg2vdec-MPEG2VideoDecoderV2InSKU"=dword:00000001

_EOF_


    w_try_regedit "$W_TMP"/mf.reg

    w_try_regedit64 "$W_TMP"/mf.reg

}




